{
  "name": "homm3-parsers",
  "version": "0.8.4",
  "description": "Collection of binary parsers able to parse various file formats related to Heroes of Might and Magic III",
  "keywords": [
    "parser",
    "binary",
    "homm3",
    "h3m",
    "lod",
    "def"
  ],
  "license": "MIT",
  "author": {
    "name": "Sergii Kostyrko",
    "email": "srg.kostyrko@gmail.com"
  },
  "main": "dist/homm3-parsers.js",
  "module": "dist/homm3-parsers.es5.js",
  "browser": "dist/homm3-parsers.bundle.js",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c && tsc --module commonjs",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepare": "yarn build",
    "lint": "eslint src/**/*.ts __tests__/**/*.ts",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/srg-kostyrko/homm3-parser.git"
  },
  "bugs": {
    "url": "https://github.com/srg-kostyrko/homm3-parser/issues"
  },
  "homepage": "https://github.com/srg-kostyrko/homm3-parser#readme",
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "binary-markup": "^0.8.6",
    "pako": "^1.0.10"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.2",
    "@types/pako": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-problems": "^3.0.1",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "rollup": "^1.16.7",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-plugin-typescript2": "^0.22.0",
    "standard-version": "^6.0.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint"
    ]
  }
}
